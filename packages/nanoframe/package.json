    "@nanoframe/core-darwin-x64": "workspace:*",
    "@nanoframe/core-darwin-arm64": "workspace:*",
    "@nanoframe/core-linux-x64": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24",
    "eslint": "9",
    "husky": "9",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
