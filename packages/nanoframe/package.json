{
  "name": "nanoframe",
  "version": "0.0.0",
  "description": "Electron-like framework using system webviews powered by a Rust core.",
  "license": "ICL",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/infinitejs/nanoframe.git",
    "directory": "packages/nanoframe"
  },
  "homepage": "https://github.com/infinitejs/nanoframe/tree/main/packages/nanoframe",
  "bugs": {
    "url": "https://github.com/infinitejs/nanoframe/issues"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "test": "vitest run",
    "prepare": "husky install"
  },
  "dependencies": {
